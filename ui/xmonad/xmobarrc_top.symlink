
Config { font = "xft:Fira Code-10"
       , bgColor = "#1C1C1C"
       , fgColor = "#4d4d4d"
       , borderColor = "#4d4d4d"
       , border = NoBorder
       , position = Top
       , lowerOnStart = True
       , hideOnStart = False
       , persistent = True
       , commands = [ Run StdinReader
                    , Run Com "xmobarrc_openvpn.sh" [] "myOpenVPN" 60
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = " %StdinReader% }{ %myOpenVPN% "
       }
